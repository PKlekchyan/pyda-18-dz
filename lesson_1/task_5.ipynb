   "source": [
    "def half_summ(n):\n",
    "    count = 0\n",
    "    while n >= 1:\n",
    "        count += n % 10\n",
    "        n = n // 10\n",
    "    return count\n",
    "\n",
    "num = int(input())\n",
    "first, second = num // 1000, num % 1000\n",
    "\n",
    "if half_summ(first) == half_summ(second):\n",
    "    print(\"Счастливый билет\")\n",
    "else:\n",
    "    print(\"Несчатстливый билет\")"
   ]
  }
